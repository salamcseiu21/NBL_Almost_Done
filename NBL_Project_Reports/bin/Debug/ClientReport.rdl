  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Ref_NBLDataSource</DataSourceName>
        <CommandText>


WITH CTE_TotalInvoice
AS
(
SELECT i.ClientId,Count(i.InvoiceRef) Total FROM  Invoice i
GROUP BY i.ClientId
)
SELECT CONCAT(c.Name,' (',ct.ClientTypeName,')') as ClientInfo,i.Total FROM CTE_TotalInvoice i,Client c ,ClientType ct
WHERE i.ClientId=c.ClientId
AND c.ClientTypeId=ct.ClientTypeId
ORDER BY i.Total DESC
</CommandText>
      </Query>
      <Fields>
        <Field Name="ClientInfo">
          <DataField>ClientInfo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
