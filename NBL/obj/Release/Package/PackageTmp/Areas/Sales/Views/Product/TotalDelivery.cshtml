
@model IEnumerable<NBL.Models.ViewModels.ViewProduct>

@{
    ViewBag.Title = "Total Delivery";
    Layout = "~/Views/Shared/_UserLayoutPage.cshtml";
}


<section class="content">
    <div class="panel panel-info">
        <div class="panel panel-heading" style="font-size: 22px;font-weight: bolder">Delivered Product</div>
        <div class="panel panel-body">
            @Html.Partial("_ViewProductSummaryPartialPage", Model)
        </div>
    </div>
</section>

@section Scripts
{
    <script>

        $('#table_product_list_in_stock').DataTable({
            "lengthMenu": [[15, 30, 50, -1], [15, 30, 50, "All"]]
        });

    </script>

}
