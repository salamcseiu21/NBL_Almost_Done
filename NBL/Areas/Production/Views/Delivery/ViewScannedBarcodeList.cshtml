@model List<NBL.Models.ViewModels.Productions.ScannedProduct>
@{
    ViewBag.Title = "ViewScannedBarcodeList";
    Layout = "~/Views/Shared/_UserLayoutPage.cshtml";
}

<section class="content">
    <h3 class="text-center text-green">Scanned Barcode List</h3>
    <div>

        @foreach (var item in Model)
        {
            @item.ProductCode<span>,</span>
        }
    </div>
</section>

